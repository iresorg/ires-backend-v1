{
  "name": "ires-backend-v1",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "seed": "tsx src/shared/database/seeder.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/shared/database/datasource.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/shared/database/datasource.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/shared/database/datasource.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.19",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^11.1.5",
    "@react-email/components": "^0.0.42",
    "@react-email/render": "^1.1.2",
    "@types/passport-jwt": "^4.0.1",
    "amqplib": "^0.10.8",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.5.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.3",
    "npm-check-updates": "^18.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "react": "19.0",
    "react-dom": "19.0",
    "react-email": "3.0.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "typeorm": "^0.3.17",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/amqplib": "^0.10.6",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.8",
    "@types/supertest": "^2.0.12",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.33.1",
    "webpack": "^5.100.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/shared/email/templates/.*$": "<rootDir>/__mocks__/reactComponentMock.js",
      "^@/(.*)$": "<rootDir>/$1",
      "^@users/(.*)$": "<rootDir>/modules/users/$1",
      "^@auth/(.*)$": "<rootDir>/modules/auth/$1",
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@agents/(.*)$": "<rootDir>/modules/agents/$1",
      "^@shared/(.*)$": "<rootDir>/shared/$1",
      "^@utils/(.*)$": "<rootDir>/utils/$1",
      "^@types/(.*)$": "<rootDir>/types/$1"
    }
  }
}
